{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": [ false ],
    "no-console": [ false ],
    "prettier": true,
    "object-literal-sort-keys": false,
    "no-string-literal": false
  },
  "rulesDirectory": []
}